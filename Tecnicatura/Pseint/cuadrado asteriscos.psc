Algoritmo sin_titulo
	Definir tam, var Como Entero; 
	Escribir "ingrese el tamaño del cuadrado";
	Leer tam;
	Para var<-0 hasta tam-1
		Escribir "*" Sin Saltar;
	FinPara
	Escribir "   ";
	Para var<-0 Hasta tam-3
		Escribir "*";
	FinPara
	Para var<-1 Hasta tam-3
		Escribir " " Sin Saltar;
	FinPara
	Escribir " ";
	Para var<-0 hasta tam-1 Con Paso 1
		Escribir "*" Sin Saltar;
	FinPara;
	Escribir "  ";
FinAlgoritmo
